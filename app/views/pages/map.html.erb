    <% if @map_route %>
    <div id="outer">

	    <div id="map-canvas" style="position: relative; left: 0px; width: 80%; height: 500px;"></div>

	      <div id="multigo" style="position: absolute; z-index: 400; right: 0px; top: 15px; width: 20%; height: 150px; background-color: teal; border-radius: 10px 10px 10px 10px">
	          <div id="upperpart" style="position: relative; background-color: white; width: 100%; height: 40px; border-radius: 10px 10px 0 0;">
	            <span style="text-align: center"><p> People Around You</p></span>
	            <% users = User.within(1, :origin => current_user) %>
	            <% users.each {|user| %>
	              <a style="margin: 5px 5px 5px 5px; text-decoration: none;" href="users/<%= user.id %>/show"> <%= user.name %> </a> </br>
	            <% } %>
	          </div>
	      </div>

      </div>
    <% end %>



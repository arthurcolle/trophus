

    <% if @map_route %>

<style>
			a:link {
				color: #84ffff;	
			}

			a:hover {
				color: #e1f5fe;	
			}

			a:visited {
				color: #1a237e;
			}

			#feeds {
				width: 100%;
				float: right;
			}


p {
	font-size: 13px;
}

.upperpart {
	position: relative;
	background-color: white;
	width: 100%;
	height: 40px;
	border-radius: 10px 10px 0 0;
}


.lowerpart {
	overflow-y: auto;
	height: 150px;
}
	      </style>

	      <style>
@media screen and (max-width: 480px){ 
	#map-canvas {
		width: 300px;
	}
}
</style>





		<div id="feeds" style="width: 100%; margin-top: 10px;">
	    	<div id="livefeed" style="position: relative; z-index: 390; height: 200px; width: 140px; background-color: teal; border-radius: 10px 10px 10px 10px">
	        	<div class="upperpart">
	            	<span style="text-align: center"><p> Dishes Around You </p></span>
		            <div class="lowerpart">
			            <% users = User.within(1, :origin => current_user) %>
			            <% users.each {|user| %>
			            <% dishes = user.dishes %>
				            <% dishes.each { |dish| %>
				            	<a style="margin: 5px 5px 5px 5px; text-decoration: none;" href="dishes/<%= dish.id %>"> <%= dish.name %> </a> </br>
				           	<% } %>
			           	<% } %>
			        </div>
				</div>
			</div>



	      <div id="multigo" style="position: relative; float: right; z-index: 400; width: 140px; height: 200px; background-color: teal; border-radius: 10px 10px 10px 10px">
	          <div class="upperpart">
	          <span style="text-align: center"><p> People Around You</p></span>
		      	<div class="lowerpart">
		            <% users = User.within(1, :origin => current_user) %>
		            <% users.each {|user| %>
		              <a class="users" style="margin: 5px 5px 5px 5px; text-decoration: none;" href="users/<%= user.id %>/show"> <%= user.name %> </a> </br>
		            <% } %>
		      	</div>
	          </div>
	      </div>




 <div id="map-canvas" style="position: absolute; right: 0; height: 500px;"></div>

<% if current_user %>
  <script>
    initialize(<%= current_user.id %>);
    $("#multigo").draggable();
    $("#livefeed").draggable();
    $("#map-canvas").draggable();
  </script>
<% end %>


    <% end %>

